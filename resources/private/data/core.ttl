<urn:uuid:718c0bbe-a89a-4115-a9d8-3a8ada9ceeff>
    a sp:Construct ;
	knowl:sparqlEndpoint <http://live.dbpedia.org/sparql> ;
	knowl:query """
		PREFIX foaf: <http://xmlns.com/foaf/0.1/>
		PREFIX dbr: <http://dbpedia.org/resource/>
		PREFIX dbo: <http://dbpedia.org/ontology/>
		PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
		PREFIX knowl: <http://knowl-edge.org/ontology/core#>
		CONSTRUCT {
		  ?resource foaf:name ?name ;
		  	dbo:birthDate ?birthDate ;
		  	dbo:deathDate ?deathDate ;
		  	dbo:abstract ?abstract ;
		  	foaf:depiction ?depiction ;
		  	dbo:successor ?successor ;
		  	knowl:externalLink ?link .
		}
		WHERE {
		  ?resource a foaf:Person ;
		  	foaf:name ?name ;
		  	dbo:birthPlace dbr:Boston ;
		  	dbo:birthDate ?birthDate ;
		  	dbo:deathDate ?deathDate ;
		  	dbo:abstract ?abstract ;
		  	dbo:thumbnail ?depiction ;
		  	foaf:isPrimaryTopicOf ?link .
		  FILTER (?birthDate < "1900-01-01"^^xsd:date)
		  FILTER (langMatches(lang(?abstract), "EN"))
		  OPTIONAL { 
		  	?resource dbo:successor ?successor
		  }
		}
		ORDER BY ?birthDate
		LIMIT 50
	""" .

